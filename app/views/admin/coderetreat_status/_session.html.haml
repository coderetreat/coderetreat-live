.session_info{id: "session_#{session.id}"}
  %p.started_at
    Started Around:
    =session.created_at.to_time.strftime("%H:%m %p")
  %p.constraints
    Constraints:
    =session.constraints
  .actions
    =link_to "Edit",
      edit_coderetreat_session_path(coderetreat_id: @coderetreat.admin_token, id: session),
      class: "edit"
    =link_to "Remove",
      coderetreat_session_path(coderetreat_id: @coderetreat.admin_token, id: @coderetreat.current_session),
      :method => :delete,
      class: "remove"
